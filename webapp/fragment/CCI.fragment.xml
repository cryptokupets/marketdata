<core:FragmentDefinition xmlns:core="sap.ui.core" 
  xmlns:chart="openui5.financial.chart" 
  xmlns:indicator="openui5.financial.chart.indicator" 
  xmlns="sap.m">
  <chart:Chart id="cci" 
    height="90px" 
    padding="5 20 25 60" 
    start="{draft>start}" 
    end="{draft>end}" 
    period="{draft>period}">
    <indicator:CCI items="{
        model: 'draft',
        path: 'Output',
        templateShareable: false
      }">
      <chart:LineChartItem time="{draft>time}" 
        value="{draft>values/0}"/>
    </indicator:CCI>
  </chart:Chart>
</core:FragmentDefinition>
