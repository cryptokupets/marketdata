<core:FragmentDefinition xmlns:core="sap.ui.core" 
  xmlns:chart="openui5.financial.chart" 
  xmlns:indicator="openui5.financial.chart.indicator" 
  xmlns="sap.m">
  <chart:Chart id="ccioz" 
    height="90px" 
    padding="5 20 25 60" 
    start="{start}" 
    end="{end}" 
    period="{period}">
    <indicator:CCIOZ items="{Output}" 
      overboughtZone="100" 
      oversoldZone="-100">
      <chart:LineChartItem time="{time}" 
        value="{values/0}"/>
    </indicator:CCIOZ>
  </chart:Chart>
</core:FragmentDefinition>
