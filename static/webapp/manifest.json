{
	"sap.app": {
		"id": "fokin.crypto",
		"type": "application",
		"dataSources": {
			"default": {
				"uri": "/odata/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5"
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "fokin.crypto.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"data": {
				"dataSource": "default",
				"settings": {
					"autoExpandSelect": true,
					"groupId": "$direct",
					"synchronizationMode": "None",
					"operationMode": "Server"
				}
			},
			"view": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"controlId": "app",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewPath": "fokin.crypto.view",
				"async": true
			},
			"routes": [
				{
					"pattern": "candles/{id}",
					"name": "candles",
					"target": "candles"
				}
			],
			"targets": {
				"candles": {
					"viewName": "Candles"
				}
			}
		}
	}
}
